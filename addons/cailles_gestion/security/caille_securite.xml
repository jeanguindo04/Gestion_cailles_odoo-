<odoo>
    <record id="module_caille" model="ir.module.category">
        <field name="name">Caille</field>
        <field name="description">Groupe de gestion des cailles</field>
        <field name="sequence">-1</field>
    </record>
    <record id="group_caille_manager" model="res.groups">
        <field name="name">cailles manager</field>
        <field name="category_id" ref="module_caille"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>